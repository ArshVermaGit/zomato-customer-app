{
  "expo": {
    "name": "Zomato",
    "slug": "zomato-customer-app",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#E23744"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zomato.customer"
    },
    "android": {
      "package": "com.zomato.customer"
    },
    "web": {},
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Zomato to use your location."
        }
      ]
    ]
  }
}